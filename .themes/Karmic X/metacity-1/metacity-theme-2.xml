      <color value="gtk:fg[NORMAL]"/>
      <color value="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.75"/>
   </gradient>

</draw_ops>

<draw_ops name="maximized_border">

   <rectangle color="gtk:fg[NORMAL]" x="0" y="0" width="width" height="height" filled="true"/>

   <!-- Highlight -->
   <gradient type="horizontal" x="0" y="1" width="width" height="1">
      <color value="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.5"/>
      <color value="blend/gtk:bg[NORMAL]/gtk:bg[SELECTED]/0.5"/>
      <color value="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.5"/>
   </gradient>

   <!-- Title -->
   <gradient type="vertical" x="0" y="2" width="width" height="16">
      <color value="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.15"/>
      <color value="gtk:fg[NORMAL]"/>
   </gradient>
   <include name="radial_gradiant"/>

</draw_ops>

<draw_ops name="maximized_border_unfocused">

   <rectangle color="gtk:fg[NORMAL]" x="0" y="0" width="width" height="height" filled="true"/>

   <!-- Highlight -->
   <gradient type="horizontal" x="0" y="1" width="width" height="1">
      <color value="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.5"/>
      <color value="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.25"/>
      <color value="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.5"/>
   </gradient>

