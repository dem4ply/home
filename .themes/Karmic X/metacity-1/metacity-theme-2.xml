      <color value="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.1"/>
      <color value="gtk:fg[NORMAL]"/>
   </gradient>

</draw_ops>

<!-- TITLE -->

<draw_ops name="title_normal">
   <title color="blend/gtk:bg[NORMAL]/gtk:fg[NORMAL]/0.01" x="3" y="(height - title_height) / 2 + 1"/> 
</draw_ops>

<draw_ops name="title_unfocused">
